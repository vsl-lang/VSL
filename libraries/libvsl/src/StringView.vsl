/**
 * An interface for browsing a string's characters.
 */
public interface StringView {
    
    /**
     * Initalizes a StringView of a string
     * 
     * @param {String} of - The string to view
     */
    public init(of string: String);
    
    /**
     * Determines the length of a string
     */
    public var length: Int { get }
    
}